{
    "name": "uttamrabadiya/api-version-manager",
    "description": "Simplify Laravel API versioning with ease. No more separate controllersâ€”just flexibility, fallbacks, and version-specific components.",
    "keywords": [
        "uttamrabadiya",
        "laravel-api-version-manager"
    ],
    "homepage": "https://github.com/uttamrabadiya/api-version-manager",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Uttam Rabadiya",
            "email": "mail@uttam.dev"
        },
        {
            "name": "Divyank Munjapara",
            "email": "mail@divyank.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Uttamrabadiya\\ApiVersionManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uttamrabadiya\\ApiVersionManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uttamrabadiya\\ApiVersionManager\\ApiVersionManagerServiceProvider"
            ]
        }
    }
}
